pipeline{
agent any
stages{
  stage('Execute Ansible'){
  steps{
         ansiblePlaybook become: true
         credentialsId: 'private_key', 
         disableHostKeyChecking: true, 
         extras: "-e variable=${directory}", 
         installation: 'ansible', 
         inventory: 'dev.inv', 
         playbook: 'filecreate.yml'
         colorized: true
     }
    }
  } 
}
