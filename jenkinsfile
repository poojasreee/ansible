pipeline{
agent any
stages{
  stage('CheckOutCode'){
    steps{
        git branch: 'main', url: 'https://github.com/poojasreee/ansible.git'
    }
  }
  stage('Execute ansible'){
    steps{
        ansiblePlaybook become: true, becomeUser: 'azureuser', credentialsId: 'ansible', disableHostKeyChecking: true, installation: 'ansible', inventory: 'dev.inv', playbook: 'filecreate.yml'
    }
  } 
}
}
