pipeline{
agent any
parameters {
    string(
      name: 'VARIABLE',
      description: 'test',
      defaultValue: 'default'
    )
stages{
  stage('Execute Ansible'){
  steps{
         ansiblePlaybook credentialsId: 'private_key', 
         disableHostKeyChecking: true, 
         extras: "-e variable=${params.VARIABLE}", 
         installation: 'ansible', 
         inventory: 'dev.inv', 
         playbook: 'filecreate.yml'
     }
    }
  } 
}
